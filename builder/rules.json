{
  "replace": {
    ":": "：",
    "G1:": "G1:\\n",
    "G2~G3:": "G2~G3:\\n",
    "op:": "op:\\n",
    "共通:": "共通:\\n",
    "+": "[+＋]",
    "-": "[-－‐-]",
    "～": "[~～]",
    "\\1\\2\\1\\3": "([+-])(\\d+～)(\\d+)|([+-])(\\d+\/)(\\d+)",
    "〇": "[○◯〇]",
    "ヒントLv": "ヒント(?:lv|レベル)",
    "Lv+\\1": "lv\\+?(\\d+)",
    "スキルPt": "スキルPt",
    "絆ゲージ+\\1": "絆(?:(?:ゲージ)?\\+(\\d)?|ゲージ(?:Lv\\+1)?$)",
    "\\1": "^([※])\\s+",
    "└": "^([└∟])\\s*",
    "『": "「",
    "』": "」",
    "·": "・"
  },
